{
  "name": "p-typewriter-monorepo",
  "type": "module",
  "version": "1.0.0",
  "description": "A monorepo for p-typewriter",
  "author": "",
  "license": "MIT",
  "keywords": [
    "components",
    "vue3"
  ],
  "scripts": {
    "start": "pnpm run dev",
    "dev": "pnpm -r --parallel run dev",
    "build": "rimraf **/dist && pnpm -r -F !play --parallel run build",
    "play": "pnpm -C play run dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "test": "vitest"
  },
  "devDependencies": {
    "@pity/eslint-config": "^0.1.9",
    "eslint": "^8.23.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3",
    "unocss": "^0.45.21",
    "vite": "^3.1.2",
    "vitest": "^0.23.4",
    "vue": "^3.2.39"
  }
}
